<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Tamagochi.Styles">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Colors.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <!--Default app brushes-->
    <SolidColorBrush x:Key="DefaultBackgroundBrush" Color="{StaticResource DefaultBackground}" />
    <SolidColorBrush x:Key="DefaultActiveBackgroundBrush" Color="{StaticResource DefaultActiveColor}" />
</ResourceDictionary>